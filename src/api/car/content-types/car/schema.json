{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "color": {
      "type": "string"
    },
    "isFeature": {
      "type": "boolean"
    },
    "brand": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::brand.brand"
    },
    "price": {
      "type": "biginteger"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "released_year": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::released-year.released-year"
    },
    "userEmail": {
      "type": "string"
    }
  }
}
